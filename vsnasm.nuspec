    <projectUrl>https://www.nasm.us/</projectUrl>
    <iconUrl>https://www.nasm.us/images/nasm.png</iconUrl>
    <developmentDependency>true</developmentDependency>
    <repository type="git" url="https://github.com/maxim-zhao/VSNASM"/>
    <description>The Netwide Assembler is an assembler for the Intel x86 architecture.

